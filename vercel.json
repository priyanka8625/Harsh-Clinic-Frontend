{
 
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vitejs/vercel"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}